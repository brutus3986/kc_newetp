DROP TABLE IF EXISTS `firewall`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `firewall` (
  `bbs_id` int(11) NOT NULL COMMENT '게시판 번호',
  `comp_name` varchar(300) NOT NULL COMMENT '회원사 이름',
  `comp_no` int(11) NOT NULL COMMENT '회원사 번호',
  `writer` varchar(20) DEFAULT NULL COMMENT '게시자',
  `story_id` int(11) NOT NULL COMMENT '게시글 번호',
  `title` varchar(300) NOT NULL COMMENT '제목',
  `policy` varchar(20) DEFAULT NULL COMMENT '정책',
  `startip` varchar(1024) NOT NULL DEFAULT '' COMMENT '포트용도',
  `startuse` varchar(30) DEFAULT NULL COMMENT '출발지용도',
  `destip` varchar(1024) NOT NULL DEFAULT '' COMMENT '포트용도',
  `destuse` varchar(30) DEFAULT NULL COMMENT '목적지용도',
  `protocol` varchar(1024) NOT NULL DEFAULT '' COMMENT '프로토콜 및 포트',
  `portuse` varchar(1024) NOT NULL DEFAULT '' COMMENT '포트용도',
  `policysdate` varchar(20) DEFAULT NULL COMMENT '정책시작일',
  `policyedate` varchar(20) DEFAULT NULL COMMENT '정책만료일',
  `contents` longtext COMMENT '비고',
  `created_at` datetime NOT NULL COMMENT '신청일자',
  `updated_at` datetime DEFAULT NULL COMMENT '수정일자',
  `status` decimal(2,0) NOT NULL DEFAULT '0' COMMENT '처리상태',
  `filename` varchar(300) NOT NULL DEFAULT '' COMMENT '업로드화일',
  `viafw` varchar(300) NOT NULL DEFAULT '' COMMENT '경유방화벽',
  PRIMARY KEY (`bbs_id`,`story_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='firewall';
/*!40101 SET character_set_client = @saved_cs_client */;

